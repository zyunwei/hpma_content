                <Button id="button_select_region_9">
                    <Label text="#xxwar_region_9" />
                    <Image class="SelectRegion" id="select_region_9" src = "file://{resources}/images/custom_game/minimap_region_state_selected/9.png"/>
                </Button>
            </Panel>
            <Panel id="RegionList" hittest="false" hittestchildren="false">
                <Image id="region_1" />
                <Image id="region_2" />
                <Image id="region_3" />
                <Image id="region_4" />
                <Image id="region_5" />
                <Image id="region_6" />
                <Image id="region_7" />
                <Image id="region_8" />
                <Image id="region_9" />
