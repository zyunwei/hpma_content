<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.css" />
		<include src="file://{resources}/styles/custom_game/avalon.css" />
		<include src="file://{resources}/layout/custom_game/frames/select_teleport/select_teleport.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/layout/custom_game/frames/select_teleport/select_teleport.js" />
	</scripts>

	<Panel hittest="false" class="SelectTeleportRoot" >
        <Image id="btnClose" src="file://{resources}/images/custom_game/cross.png" onactivate="btnClose_Click()" />
		<Panel id="SelectRegionPanel" >
			<Image class="full" src="file://{resources}/images/custom_game/minimap.png" />
			<!-- <DOTAMinimap id="SelectRegionPanel" onactivate="" renderbackground="false" hittest="true" require-composition-layer="true"/> -->
            <Panel id="SelectRegionButtonList">
                <Button id="button_select_region_1">
                    <Label text="#xxwar_region_1" />
                    <Image class="SelectRegion" id="select_region_1" src = "file://{resources}/images/custom_game/minimap_region_state_selected/1.png"/>
                </Button>
                <Button id="button_select_region_2">
                    <Label text="#xxwar_region_2" />
                    <Image class="SelectRegion" id="select_region_2" src = "file://{resources}/images/custom_game/minimap_region_state_selected/2.png"/>
