                </Button>
                <Button id="button_select_region_4">
                    <Label text="#xxwar_region_4" />
                    <Image class="SelectRegion" id="select_region_4" src = "file://{resources}/images/custom_game/minimap_region_state_selected/4.png"/>
                </Button>
                <Button id="button_select_region_5">
